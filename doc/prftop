.TH man 8 "21 August 2023" "0.0.1" "prfmon manual"

.SH NAME
  prfmon
.SH SYNOPSIS
  prfmon [-v] [--help] [--version] [-s <string>] [-e <string>]
.SH DESCRIPTION
  prfmon

  --help                    
    display this help and exit
  --version                 
    display version info and exit
  -v, --verbose             
    verbose output
  -s, --start=<string>      
    Define start tag
  -e, --end=<string>        
    Define end tag
  
.SH EXAMPLES
 
